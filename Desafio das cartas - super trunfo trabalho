#include <stdio.h>

int main() {
    // Declara√ß√£o das vari√°veis da Carta 1
    char estado1[50] = "S√£o Paulo";
    char cidade1[50] = "S√£o Paulo";
    char codigo1[9] = "01";
    float populacao1 = 10000.0;      // Popula√ß√£o menor
    float area1 = 1200.0;              // √Årea menor
    float pib1 = 100.0;                // PIB menor
    int pontos_turisticos1 = 5;        // Menos pontos tur√≠sticos

    // Declara√ß√£o das vari√°veis da Carta 2
    char estado2[50] = "Distrito Federal";
    char cidade2[50] = "Bras√≠lia";
    char codigo2[9] = "06";
    float populacao2 = 12982818.0;     // Popula√ß√£o maior
    float area2 = 5760.78;             // √Årea maior
    float pib2 = 880.0;                // PIB maior
    int pontos_turisticos2 = 16;       // Mais pontos tur√≠sticos

    // Impress√£o das cartas
    printf("\n--- CARTA 1 ---\n");
    printf("Estado: %s\n", estado1);
    printf("Cidade: %s\n", cidade1);
    printf("Popula√ß√£o: %.0f\n", populacao1);
    printf("√Årea: %.2f km¬≤\n", area1);
    printf("PIB: %.2f bilh√µes\n", pib1);
    printf("Pontos tur√≠sticos: %d\n", pontos_turisticos1);
    printf("C√≥digo: %s\n", codigo1);

    printf("\n--- CARTA 2 ---\n");
    printf("Estado: %s\n", estado2);
    printf("Cidade: %s\n", cidade2);
    printf("Popula√ß√£o: %.0f\n", populacao2);
    printf("√Årea: %.2f km¬≤\n", area2);
    printf("PIB: %.2f bilh√µes\n", pib2);
    printf("Pontos tur√≠sticos: %d\n", pontos_turisticos2);
    printf("C√≥digo: %s\n", codigo2);

    // Compara√ß√µes
    printf("\n--- COMPARA√á√ÉO DOS ATRIBUTOS ---\n");

    int pontos1 = 0, pontos2 = 0; // placar das cartas

    // Popula√ß√£o
    if (populacao1 > populacao2) {
        printf("Popula√ß√£o: Carta 1 venceu\n");
        pontos1++;
    } else if (populacao1 < populacao2) {
        printf("Popula√ß√£o: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("Popula√ß√£o: Empate\n");
    }

    // √Årea
    if (area1 > area2) {
        printf("√Årea: Carta 1 venceu\n");
        pontos1++;
    } else if (area1 < area2) {
        printf("√Årea: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("√Årea: Empate\n");
    }

    // PIB
    if (pib1 > pib2) {
        printf("PIB: Carta 1 venceu\n");
        pontos1++;
    } else if (pib1 < pib2) {
        printf("PIB: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("PIB: Empate\n");
    }

    // Pontos tur√≠sticos
    if (pontos_turisticos1 > pontos_turisticos2) {
        printf("Pontos tur√≠sticos: Carta 1 venceu\n");
        pontos1++;
    } else if (pontos_turisticos1 < pontos_turisticos2) {
        printf("Pontos tur√≠sticos: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("Pontos tur√≠sticos: Empate\n");
    }

    // Densidade populacional (menor vence)
    float densidade1 = populacao1 / area1;
    float densidade2 = populacao2 / area2;
    if (densidade1 < densidade2) {
        printf("Densidade Populacional: Carta 1 venceu\n");
        pontos1++;
    } else if (densidade1 > densidade2) {
        printf("Densidade Populacional: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("Densidade Populacional: Empate\n");
    }

    // Super Poder
    float super_poder1 = populacao1 + area1 + pib1 + pontos_turisticos1 + (1.0 / densidade1);
    float super_poder2 = populacao2 + area2 + pib2 + pontos_turisticos2 + (1.0 / densidade2);

    if (super_poder1 > super_poder2) {
        printf("Super Poder: Carta 1 venceu\n");
        pontos1++;
    } else if (super_poder1 < super_poder2) {
        printf("Super Poder: Carta 2 venceu\n");
        pontos2++;
    } else {
        printf("Super Poder: Empate\n");
    }

    // Mostrar valores dos Super Poderes
    printf("\n--- SUPER PODERES ---\n");
    printf("Carta 1: %.2f\n", super_poder1);
    printf("Carta 2: %.2f\n", super_poder2);

    // Resultado final
    printf("\n--- RESULTADO FINAL ---\n");
    printf("Placar -> Carta 1: %d | Carta 2: %d\n", pontos1, pontos2);

    if (pontos1 > pontos2) {
        printf("üèÜ Carta 1 √© a vencedora!\n");
    } else if (pontos2 > pontos1) {
        printf("üèÜ Carta 2 √© a vencedora!\n");
    } else {
        printf("‚öñÔ∏è O jogo terminou em empate!\n");
    }

    return 0;
}
